{
  "name": "expresss-ts",
  "version": "1.0.0",
  "description": "CLI to create an Express TypeScript starter project",
  "main": "dist/index.js",
  "bin": {
    "expresss-ts": "./bin/expresss-ts.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/index.ts",
